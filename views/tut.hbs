<section>
    <h2>{{title}}</h2>
    <h3>{{subtitle}}</h3>
</section>
<main>
    <section class="supplies">
        <h4>Supplies</h4>
        <ul class="supply-list">
            {{#each supplies}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <h5>Optional</h5>
        <ul class="supply-list">
            {{#each optional}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    </section>
    <section class="steps">
        <h4>Steps</h4>
        <ol>
            {{#each steps}}
            <li>{{this}} </li>
            {{/each}}
        </ol>
    </section>
</main>